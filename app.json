{
  "expo": {
    "name": "Weather Sunscreen",
    "slug": "weather-sunscreen-app",
    "description": "Your comprehensive weather companion with UV index monitoring and sunscreen recommendations",
    "version": "2.0.0",
    "newArchEnabled": true,
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "weatherApiUrl": "https://api.openweathermap.org/data/2.5",
      "uvIndexApiUrl": "https://api.openuv.io/api/v1",
      "openMeteoApiUrl": "https://api.open-meteo.com/v1"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.weather-sunscreen-app"
    }
  }
}
