{
  "cli": {
    "version": ">= 12.4.0"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.6-xcode-26.0",
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.6-xcode-26.0",
        "simulator": true,
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "image": "macos-sequoia-15.6-xcode-26.0",
        "resourceClass": "m-large"
      },
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "large"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
